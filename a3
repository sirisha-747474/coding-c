#include <math.h>

int is_armstrong(int num) {
    int original_num = num, rem, sum = 0, n = 0;
    while (original_num != 0) {
        original_num /= 10;
        n++;
    }
    original_num = num;
    while (original_num != 0) {
        rem = original_num % 10;
        sum += pow(rem, n);
        original_num /= 10;
    }
    if (sum == num) {
        return 1;
    } else {
        return 0;
    }
}

int main() {
    int start, end, i;
    printf("Enter the start of the range: ");
    scanf("%d", &start);
    printf("Enter the end of the range: ");
    scanf("%d", &end);
    printf("Armstrong numbers in the range %d to %d are:\n", start, end);
    for(i=start; i<=end; i++) {
        if(is_armstrong(i)) {
            printf("%d\n", i);
        }
    }
    return 0;
}
